<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://maven.apache.org/POM/4.0.0"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
    <parent>
        <artifactId>kt-component-web</artifactId>
        <groupId>com.kt.web</groupId>
        <version>1.0.0-SNAPSHOT</version>
    </parent>
    <modelVersion>4.0.0</modelVersion>
    <packaging>jar</packaging>

    <name>kt-component-web-core</name>
    <artifactId>kt-component-web-core</artifactId>

    <properties>
        <maven.compiler.source>8</maven.compiler.source>
        <maven.compiler.target>8</maven.compiler.target>
    </properties>

    <dependencies>

        <dependency>
            <groupId>org.springframework.boot</groupId>
            <artifactId>spring-boot-configuration-processor</artifactId>
            <scope>provided</scope>
        </dependency>

        <dependency>
            <groupId>org.springframework.boot</groupId>
            <artifactId>spring-boot-starter-test</artifactId>
            <scope>test</scope>
        </dependency>

        <dependency>
            <groupId>org.projectlombok</groupId>
            <artifactId>lombok</artifactId>
            <optional>true</optional>
        </dependency>

        <dependency>
            <groupId>com.kt.dto</groupId>
            <artifactId>kt-component-dto</artifactId>
        </dependency>

        <dependency>
            <groupId>com.kt.validator</groupId>
            <artifactId>kt-component-validator</artifactId>
        </dependency>

        <dependency>
            <groupId>com.kt.common</groupId>
            <artifactId>kt-component-common-spring-boot-starter</artifactId>
        </dependency>

        <dependency>
            <groupId>com.kt.context</groupId>
            <artifactId>kt-component-context-spring-boot-starter</artifactId>
        </dependency>

        <dependency>
            <groupId>com.kt.orm</groupId>
            <artifactId>kt-component-orm-mybatis-spring-boot-starter</artifactId>
            <scope>provided</scope>
        </dependency>

        <dependency>
            <groupId>com.kt.exception</groupId>
            <artifactId>kt-component-exception-spring-boot-starter</artifactId>
        </dependency>

        <dependency>
            <groupId>com.kt.logger</groupId>
            <artifactId>kt-component-logger-spring-boot-starter</artifactId>
        </dependency>

        <dependency>
            <groupId>com.github.xiaoymin</groupId>
            <artifactId>knife4j-spring-boot-starter</artifactId>
            <version>2.0.9</version>
        </dependency>

        <dependency>
            <groupId>org.springframework.boot</groupId>
            <artifactId>spring-boot-starter-web</artifactId>
        </dependency>

    </dependencies>

</project>